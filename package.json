    "logs": "npx serverless logs -f hello",
    "logs:t": "npx serverless logs -f hello -t",
    "test": "STAGE=DEV npx sls invoke local --function hello"
  },
  "dependencies": {
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "trello": "^0.8.0",
    "winston": "^2.4.1",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.22",
    "@types/node": "^8.0.57",
    "eslint": "^4.19.0",
    "eslint-config-typescript": "^1.0.2",
    "serverless": "^1.26.1",
    "serverless-domain-manager": "^2.3.5",
    "serverless-webpack": "5.3.1",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.2",
    "webpack": "4.35.2"
  }
}
